@model RedisProfile.ViewModels.MyProfileIndexViewModel
@{
    ViewData["Title"] = "My Profile";

    var basicData = Model.BasicData;
}

<h1>My Profile</h1>
<dl>
    <dt>User Id</dt>
    <dd>@basicData.UserId</dd>

    <dt>First Name</dt>
    <dd>@basicData.FirstName</dd>

    <dt>Last Name</dt>
    <dd>@basicData.LastName</dd>

    <dt>Email</dt>
    <dd>@basicData.Email</dd>

    <dt>Has Marketing Permission?</dt>
    <dd>@basicData.HasMarketingPermission</dd>

    <dt>Created Date</dt>
    <dd>@basicData.CreatedDate</dd>
</dl>
<table>
    <thead>
        <tr>
            <th>Id</th>
            <th>Is Closed?</th>
            <th>Submitted Date</th>
            <th>Submitter Name</th>
            <th>Description</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var inquiry in Model.SupportInquiries)
        {
        <tr>
            <td>@inquiry.Id</td>
            <td>@inquiry.IsClosed</td>
            <td>@inquiry.SubmittedDate</td>
            <td>@inquiry.SubmitterName</td>
            <td>@inquiry.Description</td>
        </tr>
        }
    </tbody>
</table>